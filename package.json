{
  "name": "@willh/speckit-initializr",
  "version": "0.1.3",
  "description": "A CLI tool to quickly reconfigure Speckit environment with different AI assistants.",
  "main": "dist/speckit-initializr.js",
  "bin": {
    "speckit-initializr": "dist/speckit-initializr.js"
  },
  "files": [
    "dist/speckit-initializr.js",
    "reconfigure-speckit.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node reconfigure-speckit.js",
    "build": "bun build reconfigure-speckit.js --target node --minify --outfile dist/speckit-initializr.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "publish:public": "npm publish --access public"
  },
  "keywords": [
    "speckit",
    "cli",
    "configuration",
    "ai",
    "assistant",
    "initializr"
  ],
  "author": "Will 保哥",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doggy8088/speckit-initializr.git"
  },
  "bugs": {
    "url": "https://github.com/doggy8088/speckit-initializr/issues"
  },
  "homepage": "https://github.com/doggy8088/speckit-initializr",
  "dependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.7",
    "shelljs": "^0.10.0"
  },
  "devDependencies": {
    "bun": "^1.3.2"
  }
}
